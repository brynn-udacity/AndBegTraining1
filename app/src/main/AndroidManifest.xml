<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.andbegtraining1" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NaturalSelection"
            android:label="NaturalSelection"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".GeneFlow"
            android:label="GeneFlow"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".GeneticDrift"
            android:label="GeneticDrift"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".BiasedMutation"
            android:label="BiasedMutation"
            android:parentActivityName=".MainActivity" >
        </activity>
    </application>

</manifest>
